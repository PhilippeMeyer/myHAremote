<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <corners
                android:radius="40dp" />
            <gradient
                android:endColor="@color/myHA_BtnGradientEnd"
                android:startColor="@color/myHA_BtnGradientEnd"
                android:type="linear" />
            <stroke
                android:width="1dp"
                android:color="@color/myHA_BtnStroke" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/myHA_BtnBackground" />
            <corners
                android:radius="40dp" />
            <stroke
                android:width="2dp"
                android:color="@color/myHA_BtnStroke2" />
        </shape>
    </item>

</layer-list>
